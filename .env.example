# Environment Variables Template
# Copy this file to .env and fill in your values

# Secret key for Flask sessions (generate a random string)
SECRET_KEY=your-secret-key-here-change-this-to-random-string

# Database URL
# For SQLite (development): sqlite:///parking.db
# For PostgreSQL (production): postgresql://user:password@host:port/database
DATABASE_URL=sqlite:///parking.db

# Flask environment
# Options: development, production
FLASK_ENV=production

# Admin credentials (optional - will use defaults if not set)
ADMIN_EMAIL=admin@parking.com
ADMIN_PASSWORD=admin123
